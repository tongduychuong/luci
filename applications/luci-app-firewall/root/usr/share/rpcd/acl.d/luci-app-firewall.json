{
	"luci-app-firewall": {
		"description": "Grant access to firewall configuration",
		"read": {
			"file": {
				"/etc/nftables.d/10-custom-filter-chains.nft": [ "read" ]
			},
			"ubus": {
				"file": [ "read" ],
				"luci": [ "getConntrackHelpers" ]
			},
			"uci": [ "firewall" ]
		},
		"write": {
			"file": {
				"/etc/nftables.d/10-custom-filter-chains.nft": [ "write" ]
			},
			"ubus": {
				"file": [ "write" ]
			},
			"uci": [ "firewall" ]
		}
	}
}
